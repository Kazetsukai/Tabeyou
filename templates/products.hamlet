
<h1>_{MsgTabeyou}

<section>
  <h2>Products

<section>
  <div class="cf">
    $forall Entity pid p <- products
      $maybe price <- productPrice p
        <a href="https://tabeyou.foxycart.com/cart?name=#{productName p}&price=#{price}">
          <div class="product">
            <div class="productName">#{productName p}
            <div class="price">$#{price}